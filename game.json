              "type": "BuiltinCommonInstructions::JsCode",
              "inlineCode": [
                "//get the x,y postition from the x,y parameter",
                "var pt = {x:0,y:0};",
                "pt.x = eventsFunctionContext.getArgument(\"TwoDX\");",
                "pt.y = eventsFunctionContext.getArgument(\"TwoDY\");",
                "",
                "//consvert from 2D to Isometric",
                "var tempPt = {x:0,y:0}",
                "tempPt.x = pt.x - pt.y;",
                "tempPt.y = (pt.x + pt.y) / 2;",
                "",
                "//set the x,y to the variable in the \"tempVariabel\" parameter",
                "const vx = eventsFunctionContext.getArgument(\"tempVariable\").getChild(\"x\");",
                "const vy = eventsFunctionContext.getArgument(\"tempVariable\").getChild(\"y\");",
                "vx.setNumber(tempPt.x);",
