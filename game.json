        {
          "description": "Iso To 2D",
          "fullName": "Iso To 2D",
          "functionType": "Action",
          "name": "IsoTo2D",
          "sentence": "Iso To 2D _PARAM1_ _PARAM2_ _PARAM3_",
          "events": [
            {
              "type": "BuiltinCommonInstructions::Standard",
              "conditions": [],
              "actions": []
            },
            {
              "type": "BuiltinCommonInstructions::JsCode",
              "inlineCode": [
                "//Get the x,y from the x,y parameters",
                "var pt = {x:0,y:0};",
                "pt.x = eventsFunctionContext.getArgument(\"IsometricX\");",
                "pt.y = eventsFunctionContext.getArgument(\"IsometricY\");",
                "",
                "//convert from Isometric to 2D",
                "var tempPt = {x:0,y:0}",
                "tempPt.x = (2 * pt.y + pt.x) / 2;",
                "tempPt.y = (2 * pt.y - pt.x) / 2;",
                "",
                "//save x,y to the variable set to the \"tempVariable\" paramet",
                "const vx = eventsFunctionContext.getArgument(\"tempVariable\").getChild(\"x\");",
                "const vy = eventsFunctionContext.getArgument(\"tempVariable\").getChild(\"y\");",
                "vx.setNumber(tempPt.x);",
                "vy.setNumber(tempPt.y);"
              ],
              "parameterObjects": "",
              "useStrict": true,
              "eventsSheetExpanded": true
            }
          ],
          "parameters": [
